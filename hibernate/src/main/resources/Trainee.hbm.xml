<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
		HBM maps the columns in Table to the properties in Bean.
		Used by Hibernate to generate SQL code
 -->
<hibernate-mapping>
	<class name="com.revature.hibernate.Trainee" table="TRAINEE">
		<id name="id" type="int">
			<column name="TRAINEE_ID"></column>
		</id>
		<property name="name" type="java.lang.String">
			<column name="TRAINEE_NAME"></column>
		</property>
		<property name="major" type="java.lang.String">
			<column name="MAJOR"></column>
		</property>	
	</class>
</hibernate-mapping>